{
  "name": "briq-builder",
  "version": "0.5.0-alpha",
  "description": "Frontend builder for the briq protocol",
  "homepage": "https://github.com/briqNFT/briq-builder/",
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir dist/ && vite build --ssr server/main.ts --outDir dist/server",
    "serve": "vite preview",
    "test": "jest src",
    "dev-server": "vite build --ssr server/main.ts --outDir dist/server -w -m development",
    "build-server": "vite build --ssr server/main.ts --outDir dist/server",
    "server": "node dist/server/main.js"
  },
  "dependencies": {
    "@jaames/iro": "^5.5.2",
    "@sentry/tracing": "^6.16.1",
    "@sentry/vue": "^6.16.1",
    "connect": "^3.7.0",
    "json-bigint": "git+https://github.com/briqNFT/json-bigint.git#fix-ts",
    "starknet": "^2.7.0",
    "three": "^0.133.1",
    "uuid": "^8.3.2",
    "vite": "^2.6.4",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/connect": "^3.4.35",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.8",
    "@types/three": "^0.134.0",
    "@types/uuid": "^8.3.3",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/test-utils": "^2.0.0-rc.16",
    "autoprefixer": "^10.4.0",
    "jest": "^26.6.3",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.2",
    "ts-jest": "^26.5.6",
    "vite-svg-loader": "^3.1.0",
    "vue-jest": "^5.0.0-alpha.10"
  }
